<persistence version="2.0"
             xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
    <persistence-unit name="gypsy_test" transaction-type="RESOURCE_LOCAL">
        <provider>com.gypsy.PersistenceProvider</provider>

        <properties>
            <property name="com.gypsy.dialect" value="com.gypsy.dialects.nosql.CouchDB"/>
            <property name="com.gypsy.loglevel" value="trace"/>
        </properties>
    </persistence-unit>



</persistence>